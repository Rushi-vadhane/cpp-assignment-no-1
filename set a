#include<iostream>
using namespace std;
int main() {
    int a, b, res;
    int ch;
    cout << "Enter 1 For Addition :";
    cout << "\nEnter 2 For Subtraction :";
    cout << "\nEnter 3 For Multiplication :";
    cout << "\nEnter 4 For Division :";
    cout << "\nEnter 5 For Modulus :"<<endl;
    cin >> ch;
    switch (ch) {
        case 1:
        {
            cout << "Enter Two Numbers :";
            cin >> a>> b;
            res = a+ b;
            cout << "\nResult is :" << res;
            break;
        }
        case 2:
        {
            cout << "Enter Two Numbers :";
            cin >> a >> b;
            res = a - b;
            cout << "Result is :" << res;
            break;
        }
        case 3:
        {
            cout << "Enter Two Numbers :";
            cin >> a>> b;
            res = a * b;
           cout << "Result is :" << res;
            break;
        }
        case 4:
        {
            cout << "Enter Two Numbers :";
            cin >> a >> b;
            res = a / b;
            cout << "Result is :" << res;
            break;
        }
        case 5:
        {
            cout << "Enter Two Numbers :";
            cin >> a >> b;
            res = a % b;
            cout << "Result is :" << res;
            break;
        }
    }

}


